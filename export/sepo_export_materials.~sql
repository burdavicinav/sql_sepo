CREATE GLOBAL TEMPORARY TABLE sepo_export_materials
(
  MAT NUMBER(7,0),
  SKL NUMBER(3,0),
  CEN NUMBER(11,2),
  POZ1 NUMBER(4,0),
  POZ2 NUMBER(4,0),
  EIO NUMBER(3,0),
  GRU NUMBER(7,0),
  NAIM VARCHAR2(20),
  MARK VARCHAR2(20),
  DIAM VARCHAR2(2),
  DM NUMBER(10,3),
  CLT NUMBER(1),
  TOL NUMBER(10,3),
  CHIR NUMBER(10,3),
  DL NUMBER(10,3),
  GOST VARCHAR2(30),
  SORM VARCHAR2(25),
  KOEF NUMBER(4,2),
  EID1 NUMBER(3,0),
  UDN1 NUMBER(8,3),
  EID2 NUMBER(3,0),
  UDN2 NUMBER(8,3),
  VED NUMBER(2,0),
  TABN NUMBER(4,0),
  ZVET VARCHAR2(10),
  SORT VARCHAR2(10),
  PPVK NUMBER(1,0),
  VPR NUMBER(1,0),
  DATV DATE,
  DKOR DATE,
  TABKOR NUMBER(7,0),
  OKEI NUMBER(5,0),
  STATUS NUMBER(1,0),
  PRIZN NUMBER(1,0),
  P_NAIM VARCHAR2(254)
) ON COMMIT PRESERVE ROWS;